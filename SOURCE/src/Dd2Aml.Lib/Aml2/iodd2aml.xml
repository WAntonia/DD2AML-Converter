            <SystemUnitClassType.InternalElement>
              <Rule>ProfileBody.DeviceFunction.Features</Rule>
              <Rule>ProfileBody.DeviceFunction.VariableCollection</Rule>
            </SystemUnitClassType.InternalElement>
          </InternalElement>
          
