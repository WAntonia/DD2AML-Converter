    <Replacement>
      <SystemUnitClassType.InternalElement>
        <InternalElement CAEXObject.Name="Features" CAEXObject.ID="Features">
          <SystemUnitClassType.Attribute>
            <Attribute CAEXObject.Name="blockParameter" AttributeDataType="xs:boolean">
